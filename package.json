{
  "name": "observable-duck",
  "version": "0.1.15",
  "description": "pure logic DuckComponent by redux & rxjs",
  "keywords": [
    "redux",
    "rxjs",
    "observable",
    "duck",
    "observable-duck"
  ],
  "files": [
    "build",
    "src",
    "typings",
    "tsconfig.json"
  ],
  "types": "./typings/index.d.ts",
  "typings": "./typings/index.d.ts",
  "main": "build/observable-duck.umd.js",
  "module": "build/observable-duck.mjs",
  "exports": {
    ".": {
      "import": "./build/observable-duck.mjs",
      "require": "./build/observable-duck.umd.js",
      "default": "./build/observable-duck.mjs"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ienyh/observable"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "tsc": "rm -rf typings && tsc",
    "build": "vite build"
  },
  "dependencies": {
    "redux": "^5.0.0",
    "redux-observable-action": "^0.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@types/redux-logger": "^3.0.12",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.1",
    "reflect-metadata": "^0.2.1",
    "reselect": "^5.0.1",
    "rxjs": "^7.8.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  }
}
